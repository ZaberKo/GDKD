    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train Detection",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/detection",
            "program": "train_net.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config-file",
                // "configs/DKD/DKD-R18-R101.yaml",
                // "configs/DKD/DKD-R50-R101.yaml",
                // "configs/ReviewKD/ReviewKD-MV2-R50.yaml",
                "configs/ReviewKD/ReviewKD-R18-R101.yaml",
                "--num-gpus",
                "1",                
                "--debug",
                "SOLVER.IMS_PER_BATCH",
                "2",
            ],
            "env": {
                // "WANDB_MODE": "offline",
                "WANDB_MODE": "disabled"
            }
        },
        {
            "name": "Train Detection Eval",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/detection",
            "program": "train_net.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config-file",
                "configs/DKD/DKD-R18-R101.yaml",
                // "configs/DKD/DKD-R50-R101.yaml",
                // "configs/ReviewKD/ReviewKD-MV2-R50.yaml",
                // "configs/ReviewKD/ReviewKD-R18-R101.yaml",
                "--num-gpus",
                "1",
                "--eval-only",                
                "OUTPUT_DIR",
                "./output/debug",
                "SOLVER.IMS_PER_BATCH",
                "2",
                "TEST.AUG.ENABLED",
                "True"
            ]
        },
        {
            "name": "train_dist",
            "type": "python",
            "request": "launch",
            "program": "train_dist.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--cfg",
                "configs/cifar100/kd.yaml",
                "--num_tests",
                "3",
                "LOG.WANDB",
                "False"
            ]
        },
        {
            "name": "logits_dist",
            "type": "python",
            "request": "launch",
            "module": "tools.statistics.calc_imagenet_teacher_logits",
            "justMyCode": true,
            "args": [
                "--dataset",
                "cifar100_aug",
                "--model",
                "resnet32x4",
            ]
        },
        {
            "name": "logits_grad",
            "type": "python",
            "request": "launch",
            "module": "exp.calc_dataset_logits_grad_dist",
            "justMyCode": true,
        },
        {
            "name": "Train ddp",
            "type": "python",
            "request": "launch",
            // "program": "tools/train.py",
            "module": "tools.train_ddp",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--cfg",
                "configs/cifar100/adkd/res32x4_res8x4.yaml",
                // "configs/cifar100/adkd/wrn40_2_shuv1.yaml",
                // "configs/cifar100/adkd/res56_res20.yaml",
                "--record_loss",
                "LOG.WANDB",
                "False",
                // "ADKD.KL_TYPE",
                // "both",
                // "LOG.ENABLE_PROGRESS_BAR",
                // "False"
            ],
            "env": {
                "LOCAL_RANK": "0",
                "RANK": "0",
                "WORLD_SIZE": "1"
            }
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            // "program": "tools/train.py",
            "module": "tools.train",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--cfg",
                // "configs/cifar100/adkd/res32x4_res8x4.yaml",
                // "configs/cifar100/reviewkd.yaml",
                // "configs/cifar100/adkd/wrn40_2_shuv1.yaml",
                // "configs/cifar100/adkd/vgg13_vgg8.yaml",
                // "configs/cifar100/adkd/res110_res32.yaml",
                // "configs/cifar100/adkd/res50_mv2.yaml",
                // "configs/cifar100/adkd/res56_res20.yaml",
                // "configs/cifar100/teacher/res32x4.yaml",
                // "configs/cifar100/gdkd/res56_res20.yaml",
                // "configs/cifar100/crd/res50_mv2.yaml",
                "configs/cifar100/reviewkd.yaml",
                "--record_loss",
                // "--teacher",
                // "ResNet50",
                // "vgg13",
                // "--student",
                // "MobileNetV2",
                // "vgg8",
                "DATASET.ENHANCE_AUGMENT",
                "False",
                "LOG.WANDB",
                "False",
                // "ADKD.KL_TYPE",
                // "both",
                // "LOG.ENABLE_PROGRESS_BAR",
                // "False"
            ]
        },
        {
            "name": "Train DKD",
            "type": "python",
            "request": "launch",
            // "program": "tools/train.py",
            "module": "tools.train",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--cfg",
                "configs/cifar100/dkdmod/res32x4_res8x4.yaml",
                "LOG.WANDB",
                "False",
            ]
        },
        {
            "name": "Train GDKDAUTOK",
            "type": "python",
            "request": "launch",
            // "program": "tools/train.py",
            "module": "tools.train",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--cfg",
                "configs/cifar100/gdkd_autok/res32x4_res8x4.yaml",
                "LOG.WANDB",
                "False",
            ]
        },
        {
            "name": "Train debug",
            "type": "python",
            "request": "launch",
            // "program": "tools/train.py",
            "module": "tools.train_debug2",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "calc grad",
            "type": "python",
            "request": "launch",
            // "program": "tools/train.py",
            "module": "tools.train_debug_dkdmod",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--top",
                "--aug",
                "none"
            ]
        }
    ]
}